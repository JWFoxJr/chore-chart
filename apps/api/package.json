{
  "name": "api",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prisma:generate": "prisma generate --config=./prisma.config.ts",
    "prisma:migrate": "prisma migrate dev --config=./prisma.config.ts",
    "prisma:reset": "prisma migrate reset --config=./prisma.config.ts",
    "prisma:seed": "prisma db seed --config=./prisma.config.ts",
    "prisma:studio": "prisma studio --config=./prisma.config.ts"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "better-sqlite3": "^12.5.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "prisma": "^7.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
